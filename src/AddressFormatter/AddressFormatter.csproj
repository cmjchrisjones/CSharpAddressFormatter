<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.0.2</Version>
    <Authors>Chris Jones</Authors>
    <Company>CJ TECH</Company>
    <Description>A library to build and format addresses. Initial release is for UK addresses, but more will be added.</Description>
