    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <RootNamespace>CJTech.AddressFormatter</RootNamespace>
    <AssemblyName>CJTech.AddressFormatter</AssemblyName>
  </PropertyGroup>

</Project>
